<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
    <!--sequence for sale.order-->
    <record id="seq_hospital_patient" model="ir.sequence">
        <field name="name">patient.sequence</field>
        <field name="code">hospital.patient.sequence</field>
        <field name="prefix">PN0</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequences for hospital.appointment -->
    <record id="seq_hospital_appointment" model="ir.sequence">
        <field name="name">appointment.Sequence</field>
        <field name="code">hospital.appointment.sequence</field>
        <field name="prefix">HP</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

        <record id="seq_job_order_line" model="ir.sequence">
        <field name="name">Job Sequence</field>
        <field name="code">hospital.job.order.sequence</field>
        <field name="prefix">JO</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>